package main;

public class Main implements Runnable {
	public Thread game;
	public void start() {
		if (game == null)
			game = new Thread(this,"game loop");
		game.start();
	}
	
	public static void init() {
		System.out.println("Ez");
	}
	public void run() {
		init();
	}
	public static void main(String[] args) {
		new Main().start();

	}

}
